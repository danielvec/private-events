<%= form_with model: @event_attending do |form| %>
  Contact Information
  <%= form.label :First name %><br>
  <%= form.text_field :title %><br>
  <%= form.label :first_name %><br>
  <%= form.text_field :first_name %><br>
  <%= form.label :last_name %><br>
  <%= form.text_field :last_name %><br>
  <%= form.label :email %><br>
  <%= form.email_field :email %><br>
  <%= form.submit "Submit" %>
<% end %>